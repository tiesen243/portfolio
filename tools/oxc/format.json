{
  "$schema": "../../node_modules/oxfmt/configuration_schema.json",
  "experimentalSortImports": {
    "newlinesBetween": true,
    "groups": [
      "side-effect",
      "type-external",
      "builtin",
      "external",
      "type-internal",
      "tsconfig-path",
      "parent",
      "sibling"
    ]
  },
  "jsxSingleQuote": true,
  "printWidth": 80,
  "semi": false,
  "singleQuote": true,
  "tabWidth": 2,
  "trailingComma": "all"
}
